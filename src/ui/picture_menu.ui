<?xml version="1.0" encoding="UTF-8"?>
<interface domain="dynamic-wallpaper-editor">
  <requires lib="gtk" version="4.0"/>
  <menu id="pic-menu">
    <section id="test">
      <item>
        <attribute name="action">win.pic_replace</attribute>
        <attribute name="label" translatable="yes">Replace withâ€¦</attribute>
      </item>
    </section>
    <section>
      <item>
        <attribute name="action">win.pic_open</attribute>
        <attribute name="label" translatable="yes">View picture</attribute>
      </item>
